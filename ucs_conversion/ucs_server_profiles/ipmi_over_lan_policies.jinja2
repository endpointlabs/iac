
  "{{ name }}" = {
    description    = "{{ descr }}"
    enabled        = {{ enabled|lower }}
    encryption_key = var.encryption_key
    privilege      = "admin"
    organization = "{{ org }}"
{%- if tags %}
    tags         = [
  {%- for item in tags %}
      {
    {%- for k, v in item.items()
        {{ k }} = {{ v }},
    {%- endfor %}
  {%- endfor %}
      },
    ]
{%- else %}
    tags         = []
{%- endif %}
  }