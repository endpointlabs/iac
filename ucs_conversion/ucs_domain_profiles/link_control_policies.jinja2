
  "{{ name }}" = {
    admin_state  = "{{ admin_state }}"
    description  = "{{ descr }}"
    mode         = "{{ mode }}"
    organization = "{{ org }}"
{%- if tags %}
    Tags         = [
  {%- for item in tags %}
      {
        key   = "{{ item['key'] }}",
        value = "{{ item['value'] }}"
      },
  {%- endfor %}
    ]
{%- else %}
    Tags         = []
{%- endif %}
  }
