
  "{{ name }}" = {
    description  = "{{ descr }}"
    priority     = "{{ priority }}"
    receive      = "{{ receive }}"
    send         = "{{ send }}"
    organization = "{{ org }}"
{%- if tags %}
    tags         = [
  {%- for item in tags %}
      {
    {%- for k, v in item.items()
        {{ k }} = {{ v }},
    {%- endfor %}
  {%- endfor %}
      },
    ]
{%- else %}
    tags         = []
{%- endif %}
  }