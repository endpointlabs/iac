
  "{{ name }}" = {
{%- if assignment_order %}
    assignment_order  = "{{ assignment_order }}"
{%- endif %}
{%- if ipv4_blocks %}
    ipv4_block         = [
  {%- for v in ipv4_blocks %}
      {
        from = "{{ v['from'] }}",
        to   = "{{ v['to'] }}"
      },
  {%- endfor %}
    ]
{%- else %}
    ipv4_block         = []
{%- endif %}
{%- if ipv4_configuration %}
    ipv4_config        = [
      {
  {%- for k, v in ipv4_configuration.items() %}
        {{ k }} = "{{ v }}",
  {%- endfor %}
      },
    ]
{%- else %}
    ipv4_config        = []
{%- endif %}
{%- if ipv6_blocks %}
    ipv6_block         = [
  {%- for v in ipv6_blocks %}
      {
        from = "{{ v['from'] }}",
        to   = "{{ v['to'] }}"
      },
  {%- endfor %}
    ]
{%- else %}
    ipv6_block         = []
{%- endif %}
{%- if ipv6_configuration %}
    ipv6_config        = [
      {
  {%- for k, v in ipv6_configuration %}
        {{ k }} = "{{ v }}",
  {%- endfor %}
      },
    ]
{%- else %}
    ipv6_config        = []
{%- endif %}
    organization      = "{{ org }}"
{%- if tags %}
    tags              = [
  {%- for item in tags %}
      {
        key   = "{{ item['key'] }}",
        value = "{{ item['value'] }}"
      },
  {%- endfor %}
    ]
{%- else %}
    tags              = []
{%- endif %}
  }