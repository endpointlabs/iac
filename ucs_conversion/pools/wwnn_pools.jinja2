
  "{{ name }}" = {
{%- if assignment_order %}
    assignment_order = "{{ assignment_order }}"
{%- endif %}
{%- if wwnn_blocks %}
    id_blocks        = [
  {%- for item in wwnn_blocks %}
      {
    {%- for k, v in item.items() %}
        {{ k }} = "{{ v }}",
    {%- endfor %}
      },
  {%- endfor %}
    ]
{%- else %}
    id_blocks        = []
{%- endif %}
    organization     = "{{ org }}"
    pool_purpose     = "WWNN"
{%- if tags %}
    tags             = [
  {%- for item in tags %}
      {
    {%- for k, v in item.items() %}
        {{ k }} = "{{ v }}",
    {%- endfor %}
      },
  {%- endfor %}
    ]
{%- else %}
    tags             = []
{%- endif %}
  }