
  "{{ name }}" = {
    classes = [
{%- for r in range(0,6) %}
      {
  {%- for k, v in classes[r].items()|sort %}
    {%- if k == 'admin_state' %}
        admin_state        = "{{ v }}"
    {%- elif k == 'bandwidth_percent' %}
        bandwidth_percent  = {{ v }}
    {%- elif k == 'cos' %}
        cos                = {{ v }}
    {%- elif k == 'mtu' %}
        mtu                = {{ v }}
    {%- elif k == 'multicast_optimize' %}
        multicast_optimize = {{ v|lower }}
    {%- elif k == 'packet_drop' %}
        packet_drop        = {{ v|lower }}
    {%- elif k == 'name' %}
        name               = "{{ v }}"
    {%- elif k == 'weight' %}
        weight             = {{ v }}
    {%- else %}
        {{ k }} = "{{ v }}"
    {%- endif %}
  {%- endfor %}
      },
{%- endfor %}
    ]
    description  = ""
    organization = "{{ org }}"
{%- if tags %}
    tags = [
  {%- for item in tags %}
      {
    {%- for k, v in item.items() %}
        {{ k }} = "{{ v }}",
    {%- endfor %}
      },
  {%- endfor %}
    ]
{%- else %}
    tags         = []
{%- endif %}
  }
