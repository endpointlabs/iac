
  "{{ name }}" = {
    description  = "{{ descr }}"
    organization = "{{ org }}"
{%- if tags %}
    tags         = [
  {%- for item in tags %}
      {
    {%- for k, v in item.items() %}
        {{ k }} = "{{ v }}",
    {%- endfor %}
      },
  {%- endfor %}
    ]
{%- else %}
    tags         = []
{%- endif %}
  }