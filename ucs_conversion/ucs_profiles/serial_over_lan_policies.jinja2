
  "{{ name }}" = {
    baud_rate    = {{ baud_rate }}
    com_port     = "{{ com_port }}"
    description  = "{{ descr }}"
    enabled      = {{ enabled|lower }}
    ssh_port     = 2400
    organization = "{{ org }}"
{%- if tags %}
    tags         = [
  {%- for item in tags %}
      {
    {%- for k, v in item.items() %}
        {{ k }} = "{{ v }}",
    {%- endfor %}
      },
  {%- endfor %}
    ]
{%- else %}
    tags         = []
{%- endif %}
  }